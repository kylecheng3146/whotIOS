# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'whotIOS' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    #beautiful textfield
    pod 'TextFieldEffects'
    #基於Alamofire的網絡抽象層
    pod 'ObjectMapper'
    pod 'RxCocoa'
    pod 'Moya/RxSwift'
    #JSON 解析
    pod 'SwiftyJSON'
    #類似Android toast 工具
    pod 'Toast-Swift'
    # Pods for whotIOS
    target 'whotIOSTests' do
        inherit! :search_paths
        # Pods for testing
        pod 'RxBlocking', '~> 4.0'
        pod 'RxTest',     '~> 4.0'
    end
    
    target 'whotIOSUITests' do
        inherit! :search_paths
        # Pods for testing
    end
end
